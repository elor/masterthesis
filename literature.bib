	pages = {1772--1784},
	file = {Avouris et al. - 2003 - Carbon nanotube electronics.pdf:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/4UMUPE8T/Avouris et al. - 2003 - Carbon nanotube electronics.pdf:application/pdf;IEEE Xplore Abstract Record:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/SZSKRJR6/abs_all.html:text/html}
