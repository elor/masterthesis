	pages = {1241--1246},
	file = {IEEE Xplore Abstract Record:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/3KBSVFT4/abs_all.html:text/html;Winstead and Ravaioli - 2000 - Simulation of Schottky barrier MOSFETs with a coup.pdf:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/WQT97WMR/Winstead and Ravaioli - 2000 - Simulation of Schottky barrier MOSFETs with a coup.pdf:application/pdf}
