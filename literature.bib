	file = {IEEE Xplore Abstract Record:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/RGIEHJVU/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/NDMV3X45/Deng and Wong - 2007 - A Compact SPICE Model for Carbon-Nanotube Field-Ef.pdf:application/pdf}
