	pages = {1280--1287},
	file = {IEEE Xplore Abstract Record:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/G7FZI8RQ/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/home/f.fuchs/.zotero/zotero/4qrndfdc.default/zotero/storage/A543SN9D/Yoon and Guo - 2007 - Analysis of Strain Effects in Ballistic Carbon Nan.pdf:application/pdf}
